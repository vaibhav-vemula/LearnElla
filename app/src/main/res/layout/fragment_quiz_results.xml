<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".QuizResultsFragment"
    android:background="@drawable/round"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottieAnimationView"
            android:layout_width="347dp"
            android:layout_height="205dp"
            android:layout_marginBottom="24dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/qu"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.703"
            app:layout_constraintStart_toStartOf="parent"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/target" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="182dp"
            android:layout_height="56dp"
            android:fontFamily="@font/alegreya_sans_sc_bold"
            android:gravity="center_horizontal"
            android:text="Results"
            android:textColor="#000000"
            android:textSize="42sp"
            app:layout_constraintBottom_toTopOf="@+id/lottieAnimationView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/qu"
            android:layout_width="297dp"
            android:layout_height="53dp"
            android:layout_marginBottom="352dp"
            android:fontFamily="@font/alegreya_sans_sc_bold"
            android:gravity="center_horizontal"
            android:lineSpacingMultiplier="1.4"
            android:text="Questions Answered"

            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/cccc"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_marginBottom="164dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/co" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/wwww"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_marginStart="80dp"

            android:layout_marginBottom="172dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.974"
            app:layout_constraintStart_toEndOf="@+id/cccc"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/wr" />

        <TextView
            android:id="@+id/correc"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="116dp"
            android:background="@drawable/correct"
            android:fontFamily="@font/alegreya_sans_sc_bold"
            android:gravity="center"
            android:text="10"
            android:textSize="52sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/wron"
            app:layout_constraintHorizontal_bias="0.578"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/wron"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginEnd="64dp"
            android:layout_marginBottom="116dp"
            android:background="@drawable/wrong"
            android:fontFamily="@font/alegreya_sans_sc_bold"
            android:gravity="center"
            android:text="9"
            android:textSize="52sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/qa"
            android:layout_width="97dp"
            android:layout_height="62dp"
            android:layout_marginBottom="328dp"
            android:fontFamily="@font/alegreya_sans_sc_bold"
            android:gravity="center_horizontal"
            android:paddingTop="-40dp"
            android:text="12"
            android:textSize="52sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>